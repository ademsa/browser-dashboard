{
  "name": "browser-dashboard",
  "homepage": "https://ademsa.github.io/browser-dashboard",
  "version": "0.3.0",
  "private": true,
  "dependencies": {
    "axios": "^0.27.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.3.0",
    "react-scripts": "^5.0.1"
  },
  "scripts": {
    "css-build": "node-sass --omit-source-map-url src/App.scss src/App.css",
    "css-watch": "npm run css-build -- --watch",
    "import-ext-data": "node src/ext-data/import.js",
    "start": "npm run css-build && react-scripts start",
    "build": "npm run css-build && react-scripts build",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "bulma": "^0.9.4",
    "node-sass": "^7.0.1"
  }
}
